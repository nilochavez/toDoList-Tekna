{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
       
        "buildCommand": "npm run build:prod",
        "distDir": "dist/frontend"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },   
    { "src": "/.*", "dest": "/index.html" } 
  ]
}
